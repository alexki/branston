<h1>New iteration</h1>

<% form_for(@iteration) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :velocity %><br />
    <%= f.text_field :velocity %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :start_date %><br />
    <%= f.calendar_date_select :start_date %>
  </p>
  <p>
    <%= f.label :end_date %><br />
    <%= f.calendar_date_select :end_date %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', iterations_path %>

